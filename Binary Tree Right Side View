class Solution {
    public List<Integer> rightSideView(TreeNode root) {
        if(root==null){
            return new ArrayList<>();
        }
        Queue<TreeNode> queue=new LinkedList<>();
        List<Integer> res=new ArrayList<>();

        queue.add(root);

        while(!queue.isEmpty()){
            int size=queue.size();
            int lastval=0;
            for(int i=0;i<size;i++){
                TreeNode node=queue.poll();
                lastval=node.val;
                if(node.left!=null){
                    queue.add(node.left);
                }
                if(node.right!=null){
                    queue.add(node.right);
                }
            }
            res.add(lastval);
        }
        return res;
    }
}
